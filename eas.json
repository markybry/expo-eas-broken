{
  "cli": {
    "version": ">= 0.45.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "CAREFOR_EMAR_ENVIRONMENT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "releaseChannel": "staging",
      "env": {
        "CAREFOR_EMAR_ENVIRONMENT": "staging"
      }
    },
    "production": {
      "releaseChannel": "production",
      "env": {
        "CAREFOR_EMAR_ENVIRONMENT": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}